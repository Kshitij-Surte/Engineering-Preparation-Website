    <script defer src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script defer src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <!-- google fonts  -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Fjalla+One&family=Girassol&family=Goblin+One&family=Pacifico&display=swap" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Lemon&family=Lemonada:wght@300&display=swap" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Goblin+One&display=swap" rel="stylesheet">
     <!-- Logo font link  -->
     <link rel="preconnect" href="https://fonts.gstatic.com">
     <link href="https://fonts.googleapis.com/css2?family=Pattaya&display=swap" rel="stylesheet">
    <!-- favicon link -->
    <link rel="shortcut icon" href="img/favicon.ico" type="image/x-icon">
    <!-- css link file  -->
    <link rel="stylesheet" href="css/home.css">
    <title>ARC</title>
  </head>
  <body>
    <nav class="navbar navbar-expand-lg bg-light" >
      <a class="navbar-brand" href="/"  style="color: #233e8b; font-family: 'Pattaya', sans-serif;">ARC</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon" style="color: #2f5d62;">=</span>
      </button>
    
      <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
        <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
          <li class="nav-item active">
            <a class="nav-link" href="/" style="color: #2f5d62">Home <span class="sr-only">(current)</span></a>
          </li>
          <li class="nav-item active">
            <a class="nav-link" href="/about" style="color: #2f5d62">About<span class="sr-only">(current)</span></a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/login" style="color: #2f5d62">Teacher login</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/announcement" style="color: #2f5d62">Announcement</a>
          </li>
          <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle" href="#" style="color: #2f5d62" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                Classes
              </a>
              <div class="dropdown-menu bg-light" style="border: none;" aria-labelledby="navbarDropdown">
                <a class="dropdown-item" href="fe">FE</a>
                <a class="dropdown-item" href="secomps">SE-Comps</a>
                <!-- <div class="dropdown-divider" ></div> -->
              </div>
            </li>
          <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle" href="#" style="color: #2f5d62" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                ChatRoom
              </a>
              <div class="dropdown-menu bg-light" style="border: none;" aria-labelledby="navbarDropdown">
                <% chatroom.forEach(room => { %>
                <a class="dropdown-item" href="/doubtroom/<%= room %>"><%= room %></a>
                <% }); %>
                <!-- <div class="dropdown-divider"></div> -->
              </div>
            </li>
        </ul>
        <form action="/search" method="POST" class="form-inline my-2 my-lg-0">
          <input class="form-control mr-sm-2" name="search" type="search" placeholder="Search">
          <button class="btn my-2 my-sm-0 btn-outline-success"  type="submit">Search</button>
        </form>
      </div>
    </nav>

    <img src="img/background.jpg" class="img-fluid bg" alt="Responsive image">

    <section id="what-we-provide-section">

      <h1 class="section-head">What we provide</h1>
  
      <div class="row section-cards">
  
        <div class=" col-lg-3 card">
          <h3 class="card-head">Prep Material</h3>
          <p class="card-body">
            We got all the material required for the preparation of various competative exams related to engineering syllabus.
          </p>
        </div>
        <div class=" col-lg-3 card">
          <h3 class="card-head">Chatroom</h3>
          <p class="card-body">
            The facility to chat helps student solve his/her doubts. The announcements helps students to know the future events from verified teachers,
          </p>
        </div>
        <div class=" col-lg-3 card">
          <h3 class="card-head">Better Interaction</h3>
          <p class="card-body">
            Guide to engineering studies and future scope.Taking interaction between student and teacher to a base level where students can connect better with teachers thus understanding the topics wisely.
          </p>
        </div>
     
      </div>
  
    </section>
    
    
    
    <section id="subjectsection">
      <h1 class="section-head">Subjects</h1>
      <div class="row">
          <div class=" col-lg-6 col-sm-4 simple" id="fe">
              <h1 class="subhead">FE</h1>
              <div class="sublist">
                  <ul>
                    <% FESubjects.forEach(subject => { %>
                      <li><a href="/<%= subject %>"><%= subject %></a></li>
                      <% }); %>
                  </ul>
              </div>
          </div>

          
          <div class=" col-lg-6 col-sm-4 simple" id="secomps">
              <h1 class="subhead">SE-Comps</h1>
              <div class="sublist">
                  <ul>
                    <% SECompsSubjects.forEach(subject => { %>
                      <li><a href="/<%= subject %>"><%= subject %></a></li>
                      <% }); %>
                  </ul>
              </div>
          </div>
      </div>


  </section>

  <fotter class="text-center">
    <!-- <div class="card-header">
      Featured
    </div> -->
    <div class="card-body">
      <% var d = new Date(); %>
      <% var n = d.getFullYear(); %>
      <!-- <h5 class="card-title"> Copyright <%= n %>.ARC  </h5> -->
      <p class="card-text" style="margin-bottom: -5px;"><span>&#169;</span> Copyright <%= n %>.ARC  </p>
      <p style="text-align: center; color: #afb9c8; font-size: 12px; margin-top: 0;">All Rights Reserved</p>
      <a href="/review" class="btn btn-outline-primary">Rate Us</a>
    </div>

  </footer>


  </body>
</html>




